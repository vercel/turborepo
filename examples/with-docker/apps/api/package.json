{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "logger": "*",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.4",
    "@types/supertest": "^2.0.16",
    "esbuild": "^0.19.7",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-config-custom-server": "*",
    "jest": "^29.7.0",
    "jest-presets": "*",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "tsconfig": "*",
    "typescript": "^5.3.2"
  }
