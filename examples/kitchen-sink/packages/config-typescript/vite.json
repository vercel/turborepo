{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": false,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "target": "ESNext",
    "types": ["vite/client"]
  }
}
