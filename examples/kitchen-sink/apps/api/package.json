    "logger": "workspace:*",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.9",
    "@types/supertest": "^2.0.12",
    "eslint-config-custom": "workspace:*",
    "jest-presets": "workspace:*",
    "supertest": "^6.3.3",
    "tsconfig": "workspace:*",
    "tsup": "^6.0.1",
    "typescript": "^5.2.2"
