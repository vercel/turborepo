    "prettier": "^3.2.5",
    "tsx": "^4.7.2"
  },
  "main": "dist/index.js",
  "napi": {
    "name": "@turbo/repository",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc",
        "aarch64-pc-windows-msvc"
