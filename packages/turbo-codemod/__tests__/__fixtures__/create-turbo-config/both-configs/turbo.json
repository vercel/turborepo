{
  "$schema": "https://turborepo.com/schema.json",
  "pipeline": {
    "turbo-only": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
