{
  "name": "no-turbo-json",
  "version": "1.0.0",
  "dependencies": {},
  "devDependencies": {
    "turbo": "1.0.0"
  },
  "turbo": {
    "$schema": "https://turborepo.com/schema.json",
    "pipeline": {
      "build": {
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "lint": {
        "outputs": []
      },
      "test": {},
      "dev": {
        "cache": false
      }
    }
  }
}
