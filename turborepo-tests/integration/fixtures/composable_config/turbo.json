      "outputs": [
        "out/**"
      ]
    },
    "cached-task-2": {
      "cache": true,
      "outputs": [
        "out/**"
      ]
    },
    "cached-task-3": {
      "outputs": [
        "out/**"
      ]
    },
    "cached-task-4": {
