{
  "name": "monorepo",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "monorepo",
      "workspaces": [
        "apps/**"
      ]
    },
    "apps/base-lib": {},
    "apps/my-app": {
      "dependencies": {
        "some-lib": "*"
      }
    },
    "apps/other-app": {
      "dependencies": {
        "yet-another-lib": "*"
      }
    },
    "apps/some-lib": {
      "dependencies": {
        "base-lib": "*"
      }
    },
    "apps/yet-another-lib": {
      "dependencies": {
        "base-lib": "*"
      }
    },
    "node_modules/base-lib": {
      "resolved": "apps/base-lib",
      "link": true
    },
    "node_modules/my-app": {
      "resolved": "apps/my-app",
      "link": true
    },
    "node_modules/other-app": {
      "resolved": "apps/other-app",
      "link": true
    },
    "node_modules/some-lib": {
      "resolved": "apps/some-lib",
      "link": true
    },
    "node_modules/yet-another-lib": {
      "resolved": "apps/yet-another-lib",
      "link": true
    }
  }
}
