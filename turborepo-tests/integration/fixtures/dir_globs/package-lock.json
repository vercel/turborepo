{
  "name": "monorepo",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "monorepo",
      "workspaces": [
        "apps/**",
        "packages/**"
      ]
    },
    "apps/my-app": {
      "dependencies": {
        "util": "*"
      }
    },
    "node_modules/my-app": {
      "resolved": "apps/my-app",
      "link": true
    },
    "node_modules/util": {
      "resolved": "packages/util",
      "link": true
    },
    "packages/util": {}
  }
}
