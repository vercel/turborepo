{
  "name": "@maxmove/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./connect": {
      "types": "./connect/index.ts",
      "default": "./connect/index.ts"
    },
    "./gateway": {
      "types": "./src/gateway/index.ts",
      "default": "./src/gateway/index.ts"
    },
    "./gen/*": {
      "types": "./gen/*.ts",
      "default": "./gen/*.ts"
    }
  },
  "scripts": {
    "build": "echo 'No build step'",
    "lint": "eslint .",
    "clean": "rm -rf node_modules bun.lock*",
    "generate": "cd ../../proto && buf generate && buf generate --template ../buf.connectquery.gen.yaml",
    "test": "bun test"
  },
  "dependencies": {
    "@bufbuild/protobuf": "catalog:",
    "@connectrpc/connect": "catalog:",
    "@connectrpc/connect-query": "catalog:",
    "@connectrpc/connect-web": "catalog:"
  },
  "peerDependencies": {
    "buffer": "^6.0.3",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "catalog:",
    "typescript-config": "workspace:*"
  }
}
