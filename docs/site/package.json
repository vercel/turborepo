{
  "name": "turborepo-docs",
  "version": "0.0.1",
  "private": true,
  "description": "The documentation for Turborepo.",
  "scripts": {
    "build": "next build",
    "check-spelling": "spellchecker --files '**/*.mdx' --files '!content/blog/*.mdx' --files '!content/repo-docs/acknowledgments.mdx' --files '!node_modules/**' --files '!content/extra/terms.mdx' --files '!content/extra/governance.mdx' --files '!content/extra/privacy.mdx' --dictionaries 'dictionary.txt' --no-suggestions --quiet",
    "dev": "next dev",
    "eslint": "fumadocs-mdx && eslint .",
    "eslint:fix": "fumadocs-mdx && eslint . --fix",
    "index-docs": "tsx ./scripts/sync-algolia.mjs",
    "rss": "node scripts/generate-rss.cjs",
    "schema": "node scripts/copy-json-schema.mjs",
    "start": "next start",
    "type-check": "fumadocs-mdx && tsc --noEmit",
    "generate-openapi": "node ./scripts/generate-docs.mjs",
    "write-private-files": "node ./scripts/write-private-files.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "1.0.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-spring/web": "^9.7.3",
    "@turbo/gen": "2.3.1-canary.2",
    "@vercel/speed-insights": "1.2.0",
    "algoliasearch": "^4.23.3",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "fast-glob": "^3.3.2",
    "flags": "3.1.1",
    "framer-motion": "12.2.0",
    "fumadocs-core": "^14.7.7",
    "fumadocs-mdx": "^11.3.1",
    "fumadocs-openapi": "^7.0.1",
    "fumadocs-ui": "^14.7.7",
    "geist": "^1.3.0",
    "lucide-react": "^0.479.0",
    "next": "15.2.1-canary.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "remark": "^15.0.1",
    "remark-mdx": "^3.1.0",
    "remark-stringify": "^11.0.0",
    "semver": "^7.3.8",
    "shiki": "^3.1.0",
    "swr": "2.2.6-beta.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@next/env": "15.2.1-canary.6",
    "@next/eslint-plugin-next": "15.2.1-canary.6",
    "@turbo/tsconfig": "workspace:^",
    "@turbo/types": "2.1.3-canary.1",
    "@types/mdx": "^2.0.8",
    "@types/node": "20.11.30",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/semver": "^7.3.13",
    "@vercel/analytics": "1.5.0",
    "@vercel/toolbar": "0.1.30",
    "autoprefixer": "^10.4.20",
    "eslint": "8.57.0",
    "gray-matter": "^4.0.3",
    "openapi-types": "^12.1.3",
    "postcss": "^8.4.35",
    "rss": "^1.2.2",
    "spellchecker-cli": "^6.2.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.6.2",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "20.x"
  }
}
